    "name": {
      "type": "string",
      "metadata": {
        "description": "The name of the analysis service."
      },
      "value": "[parameters('name')]"
    },
    "resourceId": {
      "type": "string",
      "metadata": {
        "description": "The resource ID of the analysis service."
